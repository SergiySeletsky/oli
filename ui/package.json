{
  "name": "oli-ui",
  "version": "0.1.0",
  "description": "React/Ink Terminal UI for oli",
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=23.10.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --loader tsx dist/cli.js",
    "dev": "tsx watch src/cli.ts",
    "lint": "eslint . --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "ink",
    "react",
    "terminal",
    "ai",
    "assistant"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "ink": "^5.2.0",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.30.1"
  }
}
